olumn>\n            <el-table-column label=\"挂号单号\" prop=\"oId\"></el-table-column>\n            <el-table-column label=\"患者id\" prop=\"pId\"></el-table-column>\n            <el-table-column label=\"患者姓名\" prop=\"pName\"></el-table-column>\n            <el-table-column label=\"医生姓名\" prop=\"dName\"></el-table-column>\n            <el-table-column label=\"挂号时间\" prop=\"oStart\" width=\"200px\"></el-table-column>\n            <el-table-column label=\"操作\">\n                <template slot-scope=\"scope\">\n                    <el-button type=\"warning\" style=\"font-size: 18px\" @click=\"dealClick(scope.row.oId, scope.row.pId)\">\n                        <i class=\"el-icon-monitor\" style=\"font-size: 18px\"></i>\n                         处理\n                    </el-button>\n                </template>\n            </el-table-column>\n            \n        </el-table>\n    </el-card>\n</template>\n<script>\nimport jwtDecode from \"jwt-decode\";\nimport { getToken} from \"@/utils/storage.js\";\nimport request from \"@/utils/request.js\";\nexport default {\n    name: \"orderToday\",\n    data() {\n        return {\n            userId: 1,\n            userName: \"dada\",\n            today: \"\",\n\n            orderData:[],\n                \n        }\n    },\n    methods: {\n        //挂号处理//页面跳转传值\n        dealClick(oId, pId){\n            this.$router.push(\n                {\n                    path: \"/dealOrder\",\n                    query: {\n                        oId: oId,\n                        pId: pId\n                    }\n                }\n            );\n\n        },\n        //获取挂号信息\n        requestOrder(){\n            request.get(\"doctor/findOrderByNull\", {\n                params: {\n                    dId: this.userId,\n                    oStart: this.today\n                }\n            })\n            .then(res => {\n                if(res.data.status != 200)\n                return this.$message.error(\"获取数据失败\");\n               this.orderData = res.data.data;\n                //this.orderData.dName = this.userName;\n                console.log(res.data.data);\n                  \n            })\n        },\n    //token解码\n    tokenDecode(token){\n      if (token !== null)\n      return jwtDecode(token);\n    },\n    //获取当天日期\n    nowDay(){\n        const nowDate = new Date();\n      let date = {\n        year: nowDate.getFullYear(),\n        month: nowDate.getMonth() + 1,\n        date: nowDate.getDate(),\n      };\n       if(date.date < 10){\n          date.date = \"0\"+ date.date\n      }\n      if(date.month < 10){\n        date.month = \"0\"+date.month\n      }\n      this.today = date.year+\"-\"+date.month+\"-\"+date.date;\n      \n    },\n    },\n    created(){\n        //解码token信息\n        this.userId = this.tokenDecode(getToken()).dId;\n        this.userName = this.tokenDecode(getToken()).dName;\n        console.log(this.userId);\n        console.log(this.userName);\n        //获取当天日期\n        this.nowDay();\n        //获取订单信息\n        this.requestOrder();\n       \n    },\n}\n</script>"],"mappings":"AA0BA,OAAAA,SAAA;AACA,SAAAC,QAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MAEAC,SAAA;IAEA;EACA;EACAC,OAAA;IACA;IACAC,UAAAC,GAAA,EAAAC,GAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,CACA;QACAC,IAAA;QACAC,KAAA;UACAL,GAAA,EAAAA,GAAA;UACAC,GAAA,EAAAA;QACA;MACA,CACA;IAEA;IACA;IACAK,aAAA;MACAf,OAAA,CAAAgB,GAAA;QACAC,MAAA;UACAC,GAAA,OAAAf,MAAA;UACAgB,MAAA,OAAAd;QACA;MACA,GACAe,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAnB,IAAA,CAAAoB,MAAA,SACA,YAAAC,QAAA,CAAAC,KAAA;QACA,KAAAlB,SAAA,GAAAe,GAAA,CAAAnB,IAAA,CAAAA,IAAA;QACA;QACAuB,OAAA,CAAAC,GAAA,CAAAL,GAAA,CAAAnB,IAAA,CAAAA,IAAA;MAEA;IACA;IACA;IACAyB,YAAAC,KAAA;MACA,IAAAA,KAAA,WACA,OAAA9B,SAAA,CAAA8B,KAAA;IACA;IACA;IACAC,OAAA;MACA,MAAAC,OAAA,OAAAC,IAAA;MACA,IAAAC,IAAA;QACAC,IAAA,EAAAH,OAAA,CAAAI,WAAA;QACAC,KAAA,EAAAL,OAAA,CAAAM,QAAA;QACAJ,IAAA,EAAAF,OAAA,CAAAO,OAAA;MACA;MACA,IAAAL,IAAA,CAAAA,IAAA;QACAA,IAAA,CAAAA,IAAA,SAAAA,IAAA,CAAA