path: '/sectionIndex' }\"\n                >返回科室</el-breadcrumb-item\n            >\n            <el-breadcrumb-item>{{ section }}</el-breadcrumb-item>\n        </el-breadcrumb>\n        <el-input\n            v-model=\"query\"\n            placeholder=\"请输入姓名查询\"\n            class=\"doctorInput\"\n        >\n            <el-button\n                slot=\"append\"\n                icon=\"el-icon-search\"\n                @click=\"requestDoctors\"\n            ></el-button>\n        </el-input>\n        <el-table :data=\"doctorData\" border>\n            <el-table-column\n                label=\"账号\"\n                prop=\"dId\"\n                v-model=\"doctorData.dId\"\n            ></el-table-column>\n            <el-table-column\n                label=\"姓名\"\n                prop=\"dName\"\n                v-model=\"doctorData.dName\"\n            ></el-table-column>\n            <el-table-column\n                label=\"性别\"\n                prop=\"dGender\"\n                v-model=\"doctorData.dGender\"\n            ></el-table-column>\n            <el-table-column\n                label=\"职位\"\n                prop=\"dPost\"\n                v-model=\"doctorData.dPost\"\n            ></el-table-column>\n            <el-table-column\n                label=\"部门\"\n                prop=\"dSection\"\n                v-model=\"doctorData.dSection\"\n            ></el-table-column>\n            <el-table-column label=\"操作\" prop=\"dSection\">\n                <template slot-scope=\"scope\">\n                    <el-button\n                        v-if=\"scope.row.arrangeId == null\"\n                        type=\"success\"\n                        icon=\"iconfont icon-r-paper\" \n                        style=\"font-size: 14px;\"\n                        @click=\"arrangeClick(scope.row.dId)\"\n                        > 排班</el-button\n                    >\n                    <el-button\n                        v-if=\"scope.row.arrangeId != null\"\n            